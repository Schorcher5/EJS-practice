
<%- include("partials/head") -%>

  <div class="box" id="date">
    <h1><%= date %></h1>
  </div>

  <div class="box">
    <% list.forEach((item) => { %>
    <div class="item">
      <input type="checkbox">
      <p><%= item %></p>
    </div>
    <% }); %>
    <form class="item" action="/home" method="post">
      <input type="text" name="newItem" placeholder="New Item">
      <button type="submit" name="list" value=<%= date %>>+</button>
    </form>
  </div>


<%- include("partials/footer") -%>
